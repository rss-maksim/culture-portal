{
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "array-bracket-newline": [
      1,
      {
        "minItems": 5,
        "multiline": true
      }
    ],
    "arrow-body-style": [1, "as-needed"],
    "block-spacing": 1,
    "comma-dangle": ["error", "only-multiline"],
    "curly": [2, "multi-line"],
    "func-call-spacing": [1, "never"],
    "function-paren-newline": ["error", "consistent"],
    "no-cond-assign": [1, "always"],
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [1, "all", { "ignoreJSX": "all" }],
    "no-extra-semi": "error",
    "no-func-assign": 1,
    "no-inner-declarations": [1, "both"],
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "no-unsafe-negation": 1,
    "use-isnan": 1,
    "valid-typeof": 1,
    "no-unexpected-multiline": 1,
    "block-scoped-var": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "guard-for-in": 1,
    "no-alert": 1,
    "no-caller": 1,
    "no-div-regex": 1,
    "no-else-return": 1,
    "no-eq-null": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implied-eval": 1,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-multi-str": 1,
    "no-native-reassign": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal-escape": 1,
    "no-octal": 1,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-useless-call": 1,
    "no-useless-return": 1,
    "no-void": 1,
    "no-with": 1,
    "one-var": [
      "error",
      {
        "var": "always",
        "let": "never",
        "const": "never"
      }
    ],
    "radix": 1,
    "vars-on-top": 1,
    "yoda": [
      1,
      "never",
      {
        "onlyEquality": true
      }
    ],
    "no-catch-shadow": 1,
    "no-delete-var": 1,
    "no-label-var": 1,
    "no-shadow-restricted-names": 1,
    "no-undefined": 1,
    "no-use-before-define": [1, "nofunc"],
    "array-bracket-spacing": ["error", "never"],
    "brace-style": [
      1,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "comma-spacing": [
      1,
      {
        "after": true
      }
    ],
    "comma-style": 1,
    "computed-property-spacing": [1, "never"],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": ["error", "prefer-double"],
    "key-spacing": [
      0,
      {
        "align": "colon"
      }
    ],
    "new-cap": [
      1,
      {
        "newIsCap": true,
        "capIsNew": true
      }
    ],
    "new-parens": 1,
    "no-array-constructor": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": [
      1,
      {
        "max": 2
      }
    ],
    "no-new-object": 1,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-unneeded-ternary": 1,
    "no-useless-computed-key": 1,
    "no-whitespace-before-property": 1,
    "nonblock-statement-body-position": 1,
    "object-curly-spacing": [
      1,
      "always",
      {
        "arraysInObjects": false,
        "objectsInObjects": false
      }
    ],
    "padded-blocks": [1, "never"],
    "quotes": [1, "single", "avoid-escape"],
    "quote-props": [1, "as-needed"],
    "semi-spacing": ["error", { "before": false, "after": false }],
    "semi-style": ["error", "last"],
    "semi": ["error", "never"],
    "keyword-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": ["error", "never"],
    "space-in-parens": [1, "never", {}],
    "space-infix-ops": 1,
    "space-unary-ops": [
      1,
      {
        "words": true
      }
    ],
    "spaced-comment": [1, "always"],
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "constructor-super": 1,
    "generator-star-spacing": [1, "both"],
    "no-class-assign": 1,
    "no-const-assign": 1,
    "no-this-before-super": 1,
    "object-shorthand": [1, "properties"],
    "padding-line-between-statements": [
      1,
      {
        "blankLine": "any",
        "prev": "import",
        "next": "import"
      },
      {
        "blankLine": "never",
        "prev": "const",
        "next": "const"
      },
      {
        "blankLine": "never",
        "prev": "let",
        "next": "let"
      },
      {
        "blankLine": "never",
        "prev": "var",
        "next": "var"
      }
    ],
    "prefer-const": 1,
    "prefer-spread": 1,
    "require-yield": 1,
    "max-depth": [1, 4],
    "eol-last": [1, "always"],
    "no-useless-constructor": 1
  }
}
